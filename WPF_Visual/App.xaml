<Application x:Class="WPF_Visual.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPF_Visual"
             Startup="Application_Startup">
    <Application.Resources>
        <!--#region Style Dictionaries-->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/StyleDictionaries/ButtonDictionary.xaml" />
                <!--#endregion-->
                <!--#region Color Themes Dictionaries-->
                <ResourceDictionary Source="Resources/ThemeDictionaries/DefaultColorDictionary.xaml" />
                <!--#endregion-->
            </ResourceDictionary.MergedDictionaries>
            <!--#region Image Dictionary-->
            <BitmapImage x:Key="logoXraiTransparant" UriSource="../Resources/logoXraiTransparant.png" />
            <BitmapImage x:Key="logoXraiOpaque" UriSource="../Resources/logoXraiOpaque.png" />
            <!--#endregion-->
        </ResourceDictionary>
    </Application.Resources>
</Application>